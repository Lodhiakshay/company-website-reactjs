# Application Configuration
VITE_APP_NAME=TechFlow
VITE_APP_DESCRIPTION=Professional IT Solutions Company
VITE_APP_VERSION=1.0.0
VITE_APP_URL=https://techflow.com

# API Configuration
VITE_API_BASE_URL=https://api.techflow.com
VITE_API_TIMEOUT=10000

# Contact Information
VITE_CONTACT_EMAIL=hello@techflow.com
VITE_CONTACT_PHONE=+1-555-123-4567
VITE_SUPPORT_EMAIL=support@techflow.com

# Social Media
VITE_SOCIAL_LINKEDIN=https://linkedin.com/company/techflow
VITE_SOCIAL_TWITTER=https://twitter.com/techflow
VITE_SOCIAL_GITHUB=https://github.com/techflow
VITE_SOCIAL_FACEBOOK=https://facebook.com/techflow
VITE_SOCIAL_INSTAGRAM=https://instagram.com/techflow

# Analytics
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_GTM_ID=GTM-XXXXXXX

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# External Services
VITE_RECAPTCHA_SITE_KEY=your-recaptcha-site-key
VITE_MAPS_API_KEY=your-google-maps-api-key

# Development
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info

# Email Service (for contact forms)
VITE_EMAIL_SERVICE_URL=https://api.emailjs.com/api/v1.0/email/send
VITE_EMAIL_SERVICE_ID=your-service-id
VITE_EMAIL_TEMPLATE_ID=your-template-id
VITE_EMAIL_PUBLIC_KEY=your-public-key

# File Upload
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=.pdf,.doc,.docx,.jpg,.jpeg,.png

# Rate Limiting
VITE_API_RATE_LIMIT=100
VITE_API_RATE_WINDOW=3600000

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true

# Performance
VITE_ENABLE_PWA=false
VITE_CACHE_DURATION=86400000